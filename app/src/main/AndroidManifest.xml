<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.residenciapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.VisitasActivity"></activity>
        <activity android:name=".ui.EmergenciaActivity" />
        <activity android:name=".ui.QuejasActivity" />
        <activity android:name=".ui.AvisosActivity" />
        <activity android:name=".ui.CalendarioActivity" />
        <activity android:name=".ui.AreaComunActivity" />
        <activity android:name=".ui.MenuActivity" />
        <activity android:name=".ui.RegisterActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>